-- Muestra de Rayfield Library
local Rayfield = loadstring(game:HttpGet('https://raw.githubusercontent.com/UI-Libraries/Rayfield/main/source'))()

local Window = Rayfield:CreateWindow({
    Name = "LUIS SCRIPT - MUESTRA RAYFIELD",
    AnchorPoint = Enum.AnchorPoint.BottomLeft,
    Position = UDim2.new(0, 5, 1, -5),
    AutoShow = true,
    Draggable = true,
    AlwaysOnTop = true,
    Keybind = Enum.KeyCode.RightControl
})

-- Pesta√±a de Movimiento
local MovementTab = Window:CreateTab("MOVIMIENTO ‚ö°", 4483362458) -- Icono de rayo (ID de imagen de Roblox)
MovementTab:AddSlider("WalkSpeed üèÉ", {
    Min = 16,
    Max = 300,
    Current = 16,
    Increment = 1,
    Suffix = "Studs/s",
    Callback = function(v)
        -- print("WalkSpeed cambiado a: " .. v) -- Aqu√≠ ir√≠a tu l√≥gica
        local LP = game:GetService("Players").LocalPlayer
        if LP.Character and LP.Character:FindFirstChildOfClass("Humanoid") then
            LP.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = v
        end
    end,
})

MovementTab:AddToggle("FLY MOBILE (IY) ‚úàÔ∏è", nil, function(state)
    -- print("Fly Toggled: " .. tostring(state)) -- Aqu√≠ ir√≠a tu l√≥gica del Fly
    local LP = game:GetService("Players").LocalPlayer
    local FLYING = state --
