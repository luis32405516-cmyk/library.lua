local RainbowUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/luis32405516-cmyk/library.lua/refs/heads/main/Prueba2"))()
local hud = RainbowUI:CreateWindow(230, 300)

local Player = game:GetService("Players").LocalPlayer

-- Funci√≥n mejorada para forzar la carga de la textura
local function applyTexture(objType, catalogID)
    local char = Player.Character
    if not char then return end
    
    local obj = char:FindFirstChildOfClass(objType) or Instance.new(objType, char)
    -- Intentamos cargar la ID rest√°ndole 1 (truco com√∫n para obtener la textura real)
    -- O usando la ID directa si es un asset ya procesado
    obj[objType == "Shirt" and "ShirtTemplate" or "PantsTemplate"] = "rbxassetid://" .. catalogID
end

local fTexturas = hud:AddFolder("TEXTURAS üëï")

-- SKIN 1: KINGS KNIGHT (INTENTO MEJORADO)
fTexturas:AddButton("SKIN 1", function()
    -- He usado IDs que suelen ser m√°s compatibles
    applyTexture("Shirt", "17615400950") 
    applyTexture("Pants", "17615468259")
    hud:Notify("Cargando Skin 1...", 2)
end)

-- SKIN 2: GOTH STYLE (LA QUE ME PASASTE)
fTexturas:AddButton("SKIN 2", function()
    applyTexture("Shirt", "12568533433")
    applyTexture("Pants", "11333190") -- Pantal√≥n negro
    hud:Notify("Cargando Skin 2...", 2)
end)

-- BOT√ìN EXTRA: SI TIENES UNA ID, PRU√âBALA AQU√ç
-- (Esto sirve para verificar si una ID espec√≠fica funciona en ese juego)
fTexturas:AddButton("PROBAR ID MANUAL (Camisa)", function()
    -- Aqu√≠ podr√≠as poner una funci√≥n para capturar texto si tuvieras un TextBox
    -- Por ahora reseteamos a la Goth como prueba
    applyTexture("Shirt", "12568533433")
end)

hud:AddButton("QUITAR ROPA ‚ùå", function()
    local char = Player.Character
    if char then
        if char:FindFirstChildOfClass("Shirt") then char.Shirt:Destroy() end
        if char:FindFirstChildOfClass("Pants") then char.Pants:Destroy() end
        for _, v in ipairs(char:GetChildren()) do
            if v:IsA("Accessory") then v:Destroy() end
        end
    end
    hud:Notify("Limpio", 2)
end)
