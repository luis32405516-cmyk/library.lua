-- [[ CONFIGURACIÓN ]]
local TARGET_ID = 13457104744
local TeleportService = game:GetService("TeleportService")
local Players = game:GetService("Players")
local LP = Players.LocalPlayer

-- Esperar a que el juego cargue para evitar errores de ejecución temprana
if not game:IsLoaded() then game.Loaded:Wait() end

-- LÓGICA DE SALTO AUTOMÁTICO
local function autoJump()
    -- Solo se ejecuta si NO estás ya en el juego principal
    if game.PlaceId ~= TARGET_ID then
        -- Esperamos un momento para que el mapa termine de procesar tu entrada
        task.wait(2) 
        
        -- Ejecución protegida para evitar detección por logs de error
        pcall(function()
            TeleportService:Teleport(TARGET_ID, LP)
        end)
    end
end

-- Ejecutar
autoJump()
