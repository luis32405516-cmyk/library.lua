local RainbowUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/luis32405516-cmyk/library.lua/refs/heads/main/Prueba2"))()
local hud = RainbowUI:CreateWindow(230, 380)

local events = game:GetService("ReplicatedStorage"):WaitForChild("Events")
local clickRemote = events:WaitForChild("DamageIncreaseOnClickEvent")
local winRemote = events:WaitForChild("PushEvent")

local settings = {
    autoClick = false,
    autoWin = false,
    clickDelay = 0.1,
    winDelay = 0 -- Super r√°pido como pediste
}

local fFarm = hud:AddFolder("AUTO FARM ü•ä")

-- L√≥gica de combate
local function doClick()
    task.spawn(function()
        while settings.autoClick do
            clickRemote:FireServer()
            task.wait(settings.clickDelay)
        end
    end)
end

local function doWin()
    task.spawn(function()
        while settings.autoWin do
            winRemote:FireServer(true)
            task.wait(settings.winDelay)
        end
    end)
end

fFarm:AddButton("AUTO CLICK", function()
    settings.autoClick = not settings.autoClick
    if settings.autoClick then doClick() end
end)

fFarm:AddButton("GANAR PELEA üèÜ", function()
    settings.autoWin = not settings.autoWin
    if settings.autoWin then doWin() end
end)

local fMov = hud:AddFolder("MOVIMIENTO üèÉ")

-- SLIDERS RECI√âN CREADOS
fMov:AddSlider("FUERZA SALTO", 50, 500, 50, function(v)
    local hum = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if hum then
        hum.UseJumpPower = true
        hum.JumpPower = v
    end
end)

fMov:AddSlider("VELOCIDAD", 16, 200, 16, function(v)
    local hum = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if hum then hum.WalkSpeed = v end
end)

hud:Notify("Script con Sliders Listo!", 3)
