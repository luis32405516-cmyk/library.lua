local RainbowUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/luis32405516-cmyk/library.lua/refs/heads/main/Prueba2"))()
local hud = RainbowUI:CreateWindow(230, 300)

local fFarm = hud:AddFolder("FARM AUTOM츼TICO 游볡")
local _G = _G or {}
_G.AutoPunch = false
_G.AutoHatch = false

-- Funci칩n para dar golpes autom치ticamente
fFarm:AddButton("AUTO GOLPE: OFF", function()
    _G.AutoPunch = not _G.AutoPunch
    hud:Notify(_G.AutoPunch and "Auto-Punch activado" or "Auto-Punch desactivado", 2)
    
    task.spawn(function()
        while _G.AutoPunch do
            -- Este es el evento com칰n en Punch Simulators
            game:GetService("ReplicatedStorage").Events.DamageEvent:FireServer()
            task.wait(0.1)
        end
    end)
end)

local fPets = hud:AddFolder("MASCOTAS 游")

-- Funci칩n para abrir huevos autom치ticamente
fPets:AddButton("AUTO HUEVO (B치sico)", function()
    _G.AutoHatch = not _G.AutoHatch
    hud:Notify(_G.AutoHatch and "Abriendo huevos..." or "Parado", 2)
    
    task.spawn(function()
        while _G.AutoHatch do
            -- Nota: "Huevo1" es un nombre de ejemplo, cambia seg칰n el juego
            game:GetService("ReplicatedStorage").Events.PurchaseEgg:FireServer("Huevo1")
            task.wait(1)
        end
    end)
end)

hud:AddButton("VELOCIDAD X2 (Visual)", function()
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 32
    hud:Notify("Velocidad aumentada", 2)
end)
