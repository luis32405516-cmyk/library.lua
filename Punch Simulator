local RainbowUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/luis32405516-cmyk/library.lua/refs/heads/main/Prueba2?v=" .. tick()))()
local hud = RainbowUI:CreateWindow(240, 400)

local events = game:GetService("ReplicatedStorage"):WaitForChild("Events")
local clickRemote = events:WaitForChild("DamageIncreaseOnClickEvent")
local winRemote = events:WaitForChild("PushEvent")

local state = { click = false, win = false }

local fFarm = hud:AddFolder("AUTO FARM ü•ä")
fFarm:AddButton("AUTO CLICK", function()
    state.click = not state.click
    task.spawn(function() while state.click do clickRemote:FireServer() task.wait(0.1) end end)
end)
fFarm:AddButton("GANAR PELEA üèÜ", function()
    state.win = not state.win
    task.spawn(function() while state.win do winRemote:FireServer(true) task.wait(0) end end)
end)

local fMov = hud:AddFolder("AJUSTES PERSONAJE ‚öôÔ∏è")
fMov:AddSlider("SALTO", 50, 500, 50, function(v)
    local h = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if h then h.JumpPower = v h.UseJumpPower = true end
end)
fMov:AddSlider("VELOCIDAD", 16, 250, 16, function(v)
    local h = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
    if h then h.WalkSpeed = v end
end)

hud:Notify("Movimiento y Sliders Listos ‚úÖ", 3)
