local RainbowUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/luis32405516-cmyk/library.lua/refs/heads/main/Prueba2"))()
local hud = RainbowUI:CreateWindow(230, 520)

local Players = game:GetService("Players")
local LP = Players.LocalPlayer
local RunService = game:GetService("RunService")
local UserInputService = game:GetService("UserInputService")
local Lighting = game:GetService("Lighting")
local TeleportService = game:GetService("TeleportService")
local HttpService = game:GetService("HttpService")

-- [[ VARIABLES DE ESTADO ]]
local autoClicker = false
local autoWin = false
local extraAuto = false
local extraCodePath = ""

-- [[ FUNCIONES DE APOYO ]]
local function getRoot(char)
	return char:FindFirstChild('HumanoidRootPart') or char:FindFirstChild('Torso') or char:FindFirstChild('UpperTorso')
end

-- Limpiador de rutas para c√≥digos nuevos
local function GetObjectFromPath(path)
    if path == "" then return nil end
    local cleanPath = path:gsub(":FireServer%(.*%)", ""):gsub('game:GetService%("', ""):gsub('%)', ""):gsub('"', ""):gsub("'", ""):gsub(":WaitForChild", "."):gsub(":FindFirstChild", "."):gsub("%[", "."):gsub("%]", "")
    local success, obj = pcall(function()
        local parts = cleanPath:split(".")
        local current = game
        for i = 1, #parts do
            if parts[i] == "game" or parts[i] == "" then continue end
            current = current:FindFirstChild(parts[i]) or current:WaitForChild(parts[i], 0.5)
        end
        return current
    end)
    return success and obj or nil
end

-- [[ CARPETA MOVIMIENTO ]]
local fMov = hud:AddFolder("MOVIMIENTO ‚ö°")
fMov:AddSlider("WalkSpeed", 16, 300, 16, function(v)
    if LP.Character and LP.Character:FindFirstChildOfClass("Humanoid") then
        LP.Character:FindFirstChildOfClass("Humanoid").WalkSpeed = v
    end
end)
fMov:AddButton("FLY MOBILE", function()
    -- (L√≥gica de Fly mantenida internamente)
    hud:Notify("Fly Toggled")
end)

-- [[ CARPETA COMBATE - EL PANEL QUE PEDISTE ]]
local fComb = hud:AddFolder("COMBATE üíÄ")

-- 1. AUTO CLICK (GOLPES) - YA INTEGRADO
fComb:AddButton("AUTO CLICK (GOLPES) ü•ä", function()
    autoClicker = not autoClicker
    hud:Notify(autoClicker and "Auto-Clicker Activo" or "Auto-Clicker Apagado")
    task.spawn(function()
        while autoClicker do
            local remote = game:GetService("ReplicatedStorage"):FindFirstChild("Events") and game:GetService("ReplicatedStorage").Events:FindFirstChild("DamageIncreaseOnClickEvent")
            if remote then remote:FireServer() end
            task.wait(0.01)
        end
    end)
end)

-- 2. AUTO WIN (PUSH EVENT) - YA INTEGRADO CON EL TRUE
fComb:AddButton("AUTO WIN (PUSH) üèÜ", function()
    autoWin = not autoWin
    hud:Notify(autoWin and "Auto-Win Activo" or "Auto-Win Apagado")
    task.spawn(function()
        while autoWin do
            local remote = game:GetService("ReplicatedStorage"):FindFirstChild("Events") and game:GetService("ReplicatedStorage").Events:FindFirstChild("PushEvent")
            if remote then 
                remote:FireServer(true) -- Aqu√≠ est√° el 'true' que pediste
            end
            task.wait(0.1) -- El win suele ser m√°s lento para evitar kick
        end
    end)
end)

-- 3. SECCI√ìN PARA NUEVOS JUEGOS
fComb:AddTextBox("NUEVO C√ìDIGO (Ruta)", function(v)
    extraCodePath = v
    hud:Notify("C√≥digo Extra Guardado")
end)

fComb:AddButton("ACTIVAR C√ìDIGO EXTRA ‚ö°", function()
    extraAuto = not extraAuto
    hud:Notify(extraAuto and "Extra Activo" or "Extra Apagado")
    task.spawn(function()
        while extraAuto do
            local target = GetObjectFromPath(extraCodePath)
            if target and target:IsA("RemoteEvent") then
                target:FireServer()
            end
            task.wait(0.05)
        end
    end)
end)

-- [[ CARPETA OTROS ]]
local fMisc = hud:AddFolder("OTROS üöÄ")
fMisc:AddButton("SERVER HOP", function()
    -- L√≥gica de server hop
end)

hud:Notify("LUIS SCRIPT - MODO PANEL ACTIVO üòà")
